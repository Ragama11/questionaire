<h1>Show Question</h1>

<ul>

  <li>
    <strong>Quiz:</strong>
    <%= @question.quiz_id %>
  </li>

  <li>
    <strong>Question:</strong>
    <%= @question.question %>
  </li>

  <li>
    <strong>Answer:</strong>
    <%= @question.answer %>
  </li>

  <li>
    <strong>Status:</strong>
    <%= @question.status %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.question_path(@conn, :edit, @question) %></span> |
<span><%= link "Back", to: Routes.question_path(@conn, :index) %></span>
